
<div id="background">
    <div class="buffer">
        <br>
    </div>
    <div id="content">
        <div id="logo">
            <br>
        </div>

        <div>
            <div id="Title">
                <h1>[FCLM] Fifth Column Gaming </h1>
                <h3>This page keeps track of the kills and deaths of [FCLM] players</h3>
            </div>
        </div>
        <h3>FCLM Players:</h3>
        <div id="outfit" class="tableDiv">
            <table id="outfitTable" class="sortable">
                <thead>
                    <tr>
                        <th style="width:10%">DA</th>
                        <th style="width:10%">FISU</th>
                        <th style="width:20%">Player</th>
                        <th style="width:10%">Battle Rank</th>
                        <th style="width:10%">Kills</th>
                        <th style="width:10%">Deaths</th>
                        <th style="width:10%">KDR</th>
                        <th style="width:10%">HSR</th>
                    </tr>
                </thead>
                <tbody>
                {{#each tracked}}
                    <tr class="faction1">
                        <td><a href="http://stats.dasanfall.com/ps2/player/{{this.name}}">Stats</a></td>
                        <td><a href="http://ps2.fisu.pw/player/?name={{this.name}}">Stats</a></td>
                        <td>{{this.name}}</td>
                        <td>{{this.rank}}</td>
                        <td>{{this.kills}}</td>
                        <td>{{this.deaths}}</td>
                        <td>{{kdr this.kills this.deaths}}</td>
                        <td>{{hsr this.kills this.headshots}}</td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        </div>
        <br>
        <div class="inline">
            <h3 id="killsTitle">Players FCLM kills:</h3>
            <div id="kills" class="tableDiv">
                <table id="killsTable" class="sortable">
                    <thead>
                        <tr>
                            <th style="width:10%">DA</th>
                            <th style="width:10%">FISU</th>
                            <th style="width:30%">Player</th>
                            <th style="width:20%">Outfit</th>
                            <th style="width:10%">Kills</th>
                            <th style="width:10%">Deaths</th>
                            <th style="width:10%">K/D</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each characters}}
                            {{#if this.deaths}}
                            <tr class="faction{{this.faction}}">
                                <td><a href="http://stats.dasanfall.com/ps2/player/{{this.name}}">Stats</a></td>
                                <td><a href="http://ps2.fisu.pw/player/?name={{this.name}}">Stats</a></td>
                                <td>{{this.name}}</td>
                                <td>[{{this.outfit_alias}}]</td>
                                <td>{{this.kills}}</td>
                                <td>{{this.deaths}}</td>
                                <td>{{kdr this.kills this.deaths}}</td>
                            </tr>
                            {{/if}}
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
        <p class="smallBreak"></p>
        <div class="inline">
            <h3 id="killsOutfitTitle">Outfits FCLM kills:</h3>
            <div id="killsOutfit" class="tableDiv">
                <table id="killsOutfitTable" class="sortable">
                    <thead>
                    <tr>
                        <th style="width:10%">DA</th>
                        <th style="width:10%">FISU</th>
                        <th style="width:10%">Alias</th>
                        <th style="width:20%">Outfit</th>
                        <th style="width:10%">Kills</th>
                        <th style="width:10%">Deaths</th>
                        <th style="width:10%">K/D</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each outfits}}
                        {{#if this.deaths}}
                            <tr class="faction{{this.faction}}">
                                <td><a href="http://stats.dasanfall.com/ps2/outfit/{{this.alias}}">Stats</a></td>
                                <td><a href="http://ps2.fisu.pw/outfit/?name={{this.alias}}">Stats</a></td>
                                <td>{{this.alias}}</td>
                                <td>{{this.name}}</td>
                                <td>{{this.kills}}</td>
                                <td>{{this.deaths}}</td>
                                <td>{{kdr this.kills this.deaths}}</td>
                            </tr>
                        {{/if}}
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
        <br>
        <div class="inline">
            <h3 id="deathsTitle">Players FCLM dies to:</h3>
            <div id="deaths" class="tableDiv">
                <table id="deathsTable" class="sortable">
                    <thead>
                        <tr>
                            <th style="width:10%">DA</th>
                            <th style="width:10%">FISU</th>
                            <th style="width:30%">Player</th>
                            <th style="width:20%">Outfit</th>
                            <th style="width:10%">Kills</th>
                            <th style="width:10%">Deaths</th>
                            <th style="width:10%">K/D</th>
                        </tr>
                    </thead>
                    <tbody>
                    {{#each characters}}
                        {{#if this.kills}}
                        <tr class="faction{{this.faction}}">
                            <td><a href="http://stats.dasanfall.com/ps2/player/{{this.name}}">Stats</a></td>
                            <td><a href="http://ps2.fisu.pw/player/?name={{this.name}}">Stats</a></td>
                            <td>{{this.name}}</td>
                            <td>[{{this.outfit_alias}}]</td>
                            <td>{{this.kills}}</td>
                            <td>{{this.deaths}}</td>
                            <td>{{kdr this.kills this.deaths}}</td>
                        </tr>
                        {{/if}}
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
        <p class="smallBreak"></p>
        <div class="inline">
            <h3 id="deathsOutfitTitle">Outfits FCLM dies to:</h3>
            <div id="deathsOutfit" class="tableDiv">
                <table id="deathsOutfitTable" class="sortable">
                    <thead>
                    <tr>
                        <th style="width:10%">DA</th>
                        <th style="width:10%">FISU</th>
                        <th style="width:10%">Alias</th>
                        <th style="width:20%">Outfit</th>
                        <th style="width:10%">Kills</th>
                        <th style="width:10%">Deaths</th>
                        <th style="width:10%">K/D</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each outfits}}
                        {{#if this.kills}}
                            <tr class="faction{{this.faction}}">
                                <td><a href="http://stats.dasanfall.com/ps2/outfit/{{this.alias}}">Stats</a></td>
                                <td><a href="http://ps2.fisu.pw/outfit/?name={{this.alias}}">Stats</a></td>
                                <td>{{this.alias}}</td>
                                <td>{{this.name}}</td>
                                <td>{{this.kills}}</td>
                                <td>{{this.deaths}}</td>
                                <td>{{kdr this.kills this.deaths}}</td>
                            </tr>
                        {{/if}}
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
        <div class="inline">
            <h3 id="weaponsKillsTitle">Weapons used for FCLM kills:</h3>
            <div id="weaponsKills" class="tableDiv">
                <table id="weaponsKillsTable" class="sortable">
                    <thead>
                    <tr>
                        <th>#</th>
                        <th>Weapon</th>
                        <th>Kills</th>
                        <th>Headshots</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each weapons}}
                        {{#if this.kills}}
                            <tr>
                                <td>{{this.weapon_id}}</td>
                                <td>{{this.name}}</td>
                                <td>{{this.kills}}</td>
                                <td>{{this.headshots}}</td>
                            </tr>
                        {{/if}}
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
        <p class="smallBreak"></p>
        <div class="inline">
            <h3 id="weaponsDeathsTitle">Weapons used in FCLM deaths:</h3>
            <div id="weaponsDeaths" class="tableDiv">
                <table id="weaponsDeathsTable" class="sortable">
                    <thead>
                    <tr>
                        <th>#</th>
                        <th>Weapon</th>
                        <th>Kills Against FCLM</th>
                        <th>Headshots</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each weapons}}
                        {{#if this.deaths}}
                            <tr>
                                <td>{{this.weapon_id}}</td>
                                <td>{{this.name}}</td>
                                <td>{{this.deaths}}</td>
                                <td>{{this.headshots}}</td>
                            </tr>
                        {{/if}}
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
        <br>
        <h3 id="baseTitle">FCLM Base Captures:</h3>
        <div id="bases" class="tableDiv">
            <table id="baseTable" class="sortable">
                <thead>
                <tr>
                    <th>#</th>
                    <th>Base</th>
                    <th>Type</th>
                    <th>Captures</th>
                </tr>
                </thead>
                <tbody>
                {{#each bases}}
                    <tr>
                        <td>{{this.base_id}}</td>
                        <td>{{this.base_name}}</td>
                        <td>{{this.base_type}}</td>
                        <td>{{this.captures}}</td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        </div>
        <br>
        <br>
        <br>
    </div>
    <div class="buffer">
        <br>
    </div>
</div>